{
    "name": "Smart Choice",
    "description": "Give advices to users about marfa",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
    "16": "css/assets/icon16.png",
    "48": "css/assets/icon48.png",
    "128": "css/assets/icon128.png"
  },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "http://192.168.234.4:5000/"
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "webNavigation",
        "contextMenus",
        "storage"
    ],
    "background":{
        "service_worker": "js/background.js"
    }

}